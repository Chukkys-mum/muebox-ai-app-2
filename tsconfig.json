{
  "extends": "./tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/supabase/*": [
        "utils/supabase/*"
      ],
      "@/types/*": [
        "types/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "types": [
      "node",
      "react",
      "react-dom",
      "d3"
    ],
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "utils/supabase/server.prod"
  ],
  "exclude": [
    "node_modules"
  ]
}
